<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Dream Portal Test Regression Suite" thread-count="1"
	parallel="tests" verbose="1">
	<listeners>
		<listener
			class-name="dream.qa.dreamportal.listeners.TestAllureListener" />
		<listener
			class-name="dream.qa.dreamportal.listeners.ExtentReportListener" />
		<listener
			class-name="dream.qa.dreamportal.listeners.AnnotationTransformer" />
	</listeners>

	<test name="Dreams Diary Page Test test">
		<classes>
			<class name="dream.qa.dreamportal.tests.DreamsDiaryPageTest" />
		</classes>
	</test>

</suite>